{
  "name": "association-map-form",
  "version": "0.9.35",
  "scripts": {
    "app-form:ng": "ng",
    "app-form:start": "ng serve",
    "app-form:start:parallel": "ng serve --port 4100",
    "app-form:build": "ng build",
    "app-form:build:prod": "ng build --configuration production --base-href /edit/",
    "postapp-form:build:prod": "npm-run-all --continue-on-error -l --sequential copy-api-folder delete-api-folder",
    "copy-api-folder": "robocopy \"../dist/AssociationMap/edit/api\" \"../dist/AssociationMap/api\" *.* /S /E /IS",
    "delete-api-folder": "del-cli --force \"../dist/AssociationMap/edit/api\"",
    "app-form:version": "npm version patch --no-git-tag-version",
    "app-form:version:commit": "npm version patch -m \"Upgrade app-form to %s\"",
    "app-form:test": "ng test",
    "app-form:lint": "ng lint",
    "app-form:e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.4.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.4.0",
    "@angular/compiler": "^13.4.0",
    "@angular/core": "^13.4.0",
    "@angular/forms": "^13.4.0",
    "@angular/localize": "~13.4.0",
    "@angular/platform-browser": "^13.4.0",
    "@angular/platform-browser-dynamic": "^13.4.0",
    "@angular/router": "^13.4.0",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "@types/ol": "^6.5.3",
    "@types/uuid": "^8.3.1",
    "angular-csv-ext": "^1.0.5",
    "bootstrap": "^4.5.0",
    "del-cli": "^3.0.1",
    "file-saver": "^2.0.5",
    "ngx-cookie-service": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "ol": "^6.9.0",
    "ol-geocoder": "4.1.2",
    "primeicons": "^5.0.0",
    "primeng": "^13.4.1",
    "resize-observer": "^1.0.4",
    "rxjs": "^7.5.0",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.11",
    "@angular/cli": "^13.3.11",
    "@angular/compiler-cli": "^13.3.11",
    "@types/jasmine": "^3.6.11",
    "@types/node": "^12.20.16",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "^4.5.5"
  }
}
